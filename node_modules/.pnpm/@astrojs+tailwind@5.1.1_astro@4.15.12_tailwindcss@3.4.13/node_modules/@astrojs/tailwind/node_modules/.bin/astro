#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/pablojavier/Documents/Development/new projects/test-3d/node_modules/.pnpm/astro@4.15.12_typescript@5.6.2/node_modules/astro/node_modules:/Users/pablojavier/Documents/Development/new projects/test-3d/node_modules/.pnpm/astro@4.15.12_typescript@5.6.2/node_modules:/Users/pablojavier/Documents/Development/new projects/test-3d/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/pablojavier/Documents/Development/new projects/test-3d/node_modules/.pnpm/astro@4.15.12_typescript@5.6.2/node_modules/astro/node_modules:/Users/pablojavier/Documents/Development/new projects/test-3d/node_modules/.pnpm/astro@4.15.12_typescript@5.6.2/node_modules:/Users/pablojavier/Documents/Development/new projects/test-3d/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@4.15.12_typescript@5.6.2/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@4.15.12_typescript@5.6.2/node_modules/astro/astro.js" "$@"
fi
